# Database Configuration
DATABASE_URL=postgresql://username:password@your-neon-db-url

# Qdrant Configuration
QDRANT_URL=https://your-qdrant-cluster.qdrant.tech
QDRANT_API_KEY=your-qdrant-api-key

# Google AI Configuration - this is the critical one that must be set
GOOGLE_API_KEY=your-google-ai-api-key
GOOGLE_MODEL=gemini-pro

# OpenAI Configuration (for backward compatibility)
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=gpt-4-turbo

# Application Configuration
DEBUG=false
HOST=0.0.0.0
PORT=8000
SECRET_KEY=your-production-secret-key

# API settings
ALLOWED_ORIGINS=https://your-username.github.io

# Performance settings
RESPONSE_MAX_TOKENS=1000
EMBEDDING_CHUNK_SIZE=1000